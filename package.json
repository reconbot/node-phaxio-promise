{
  "author": {
    "name": "Francis Gulotta",
    "email": "wizard@roborooter.com",
    "url": "https://roborooter.com"
  },
  "name": "phaxio-promise",
  "description": "Send faxes with the Phaxio API and promises",
  "license": "MIT",
  "keywords": [
    "fax",
    "phaxio",
    "promises"
  ],
  "scripts": {
    "unit": "mocha --require test/setup.js --recursive 'test/*-test.js'",
    "lint": "eslint ./",
    "test": "npm run lint && npm run unit",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "version": "0.0.0-development",
  "homepage": "https://github.com/reconbot/node-phaxio-promise",
  "repository": {
    "type": "git",
    "url": "https://github.com/reconbot/node-phaxio-promise.git"
  },
  "engines": {
    "node": ">=4.4.x",
    "npm": ">=3.8.x"
  },
  "main": "./src/phaxio.js",
  "dependencies": {
    "form-data": "^2.1.2",
    "got": "^6.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "busboy": "^0.2.12",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "mocha": "^3.2.0",
    "nock": "^9.0.5",
    "semantic-release": "^15.1.7"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {"type": "docs", "scope": "README", "release": "patch"},
        {"type": "chore", "release": "patch"},
        {"type": "refactor", "release": "patch"}
      ]
    }
  }
}
